
import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
       Scanner scan=new Scanner(System.in);
        int s=scan.nextInt();
        System.out.println("Bird said:"+s);
        int x=s/10;
        int y=s%10;
        int z=x+y;
        System.out.println("Alice must go in path-"+z);
        
}}
